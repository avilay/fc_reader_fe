<form role="form" action="/feeds/add" method="GET">
  <div class="form-group">
    <label for="idFeedUrl">Url</label> 
    <input type="text" class="form-control" id="idFeedUrl" name="feed_url">
  </div>
  <input type="submit" class="btn btn-primary" value="Add" />
</form>
<p>&nbsp;</p>
<p>&nbsp;</p>
<blockquote>
  <p>Or add multiple feeds at once</p>
</blockquote>
<form role="form" action="/feeds/bulk_add" enctype="multipart/form-data" method="POST">        
  <div class="form-group">
    <label for="opmlFile">Bulk upload</label>
    <input type="file" id="opmlFile" name="opml_file">
    <p class="help-block">Upload the OPML file containing a list of your subscribed feeds</p>
  </div>      
  <input type="submit" class="btn btn-default" value="Upload" />
</form>
